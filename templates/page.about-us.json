    "custom_slide_show_hero_whLp6K": {
      "type": "custom-slide-show-hero",
      "blocks": {
        "slide_4TrYpV": {
          "type": "slide",
          "settings": {
            "image": "shopify:\/\/shop_images\/Kirsche_rot.jpg",
            "heading": "OUR COFFEE SOURCING",
            "heading_1": "",
            "heading_2": "",
            "heading_size": "h1",
            "subheading": "We would be nothing without our dedicated farmers growing some of the best coffees in the world. By working closely with strong local partners we build trusted relationships with outstanding producers. We give each of them a face to the market with every bag of roasted beans we produce.",
            "subheading_2": "",
            "button_label": "",
            "link": "",
            "button_style_secondary": false,
            "button_label_2": "",
            "link_2": "",
            "button_style_secondary_2": false,
