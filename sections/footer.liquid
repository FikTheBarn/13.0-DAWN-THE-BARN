{%- style -%}


  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }

  @media screen and (min-width: 750px) {
  

    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }
.newsletter-text-button-form-container{
  background-color:transparent;
}

.newsletter-container{
  position:relative;
  height:auto;
  margin-top:0rem;
  padding: 0px;
}
.newsletter-content-container{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items:flex-start;
  position:absolute;
  top:0%;
  width:100%;
  height:455px;

}
.newsletter-text-container{

  color:white;
}
.klaviyo-form{
  padding:0px !important;
}
.newsletter-title {
  font-size:42px;
  color:white;
  letter-spacing: 2px;
  margin:0px !important;

}

.text-newsletter{
  margin-top:5rem;
}
.newsletter-buttons-container{

  padding-bottom:1rem;
  color:white;

}
.newsletter-form-container{
  display:flex;
  flex-direction:column;
  align-items:flex-end;
  width:100%;

}
.newsletter-buttons-container form {
  display:flex;
  flex-direction:row;
  padding:0px;

}
input[type="radio"] + label {
  cursor: pointer;
}
input[type="radio"] {
  accent-color: black; 
}
.newsletter-image-container{
  width:100%;
  height:455px;
}
.newsletter-image-container::after {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 50%; /* Shadow covers 20% of the image height from the top */
    background: linear-gradient(to bottom, rgba(0, 0, 0, 0.9), transparent); /* 90% black to transparent */
    pointer-events: none;
}
.image-newsletter{
  width:100%;
  height:100%;
  object-fit:cover;
  object-position: right 20px;
}
.languageSelectionForm{
    width:400px;
  }

 {%comment%} Small screen {%endcomment%}
 @media screen and (max-width: 750px) {
 

   .newsletter-content-container{
     position:absolute;
     top:0px;
     right:0;
     left:0;
     display: flex;
     flex-direction: column;
     align-items: center;

   }
   .newsletter-title-container{
    text-align:center;
   }
  .newsletter-title{
    font-size:32px;
  }
  .image-newsletter {
    width: 100%;
    height: 100%;
    object-fit: cover;
    object-position: center top; /* Adjusts the position to be at the top center */

  } 
  .newsletter-text-container {
    padding-right:0rem;
    text-align:center;
  }
  .text-newsletter{
    margin-top:4rem;
  }
  .newsletter-buttons-container form {
    display:flex;
    flex-direction:row;
    justify-content:center;
  }
  .newsletter-buttons-container{

    padding-right:0rem;

  }
  .newsletter-form-container{
    padding:0;
    display:flex;
    flex-direction:column;
    align-items:center;
  }

  .languageSelectionForm{

    width:100%;
  }
}

{% comment %} //////////////////ACCORDION  SECTION///////////////////// {% endcomment %}


  .master-section-container-accordion{
    display: flex;
    flex-direction: column;
    border:20px solid var(--padding-sections);
  }
  .accordion-button {
      display: flex;
      justify-content: space-between;
      align-items: center;
      padding: 18px;
      width: 100%;
      height:calc(70vh / 9);
      background-color: #141414;
      font-size:20px !important;
      color: white;
      border: none;
      outline: none;
      transition: 0.4s;
      cursor: pointer;
      border-bottom: 2px solid var(--padding-sections);
      text-align:left;
      font-family:'Chalet Comprime';
      height: 6rem;

  }
  .accordion-button p {
    font-size:2.8rem !important;

  }
  .button-text {
      flex-grow: 1; /* Ensures text takes up available space, pushing the arrow to the right */
  }
  .arrow {
      transition: transform 0.3s ease; /* Ensures smooth rotation */
      display: inline-block; /* Helps manage transformations better */
  }
  .arrow.up {
      transform: rotate(-180deg); /* Ensures the arrow points up */
  }
  .accordion-button:hover, .accordion-button:active {
      background-color: #141414;
  }
  .panel {
      padding: 0 18px;
      background-color: #222222;
      max-height: 0;
      overflow: hidden;
      transition: max-height 0.2s ease-out;
      color:black;
  }
  .panel p {
    padding-left:2rem;
  }
  @media (max-width: 1100px) {

  .master-container-button-mobile-accordion{
        display:flex;
        flex-direction:column;
        justify-content:center;
        text-align:center;
        align-items:center;
        height:10%;
        border-top:20px solid var(--padding-sections);
        border-bottom:20px solid var(--padding-sections);
        padding:40px;
        background-color:black;
    
      }
  }

  .cta-bottom-container-desktop{
    display:flex;
    flex-direction:column;
    justify-content:center;
    align-items:center;
    padding-top:3rem;
  }

  .footer-subtitle-mobile{
      color:blue;
  }

  {% comment %} //////////////////MOBILE///////////////////// {% endcomment %}
  @media (max-width: 1100px) {

    .footer-mobile {
      display:block;
      margin-top:3rem;
    }
    .footer-desktop {
      display:none;
    }
    .mobile-logo-socialm-copy-container {
      background-color:#141414;
      display:flex;
      flex-direction:column;
      justify-content:center;
      align-items:center;
      padding-top:6rem;
      padding-bottom:3rem;

    }
    .mobile-logo-container{
      height:9rem;
      width:100%;
      background-color:#141414;
      display:flex;
      flex-direction:column;
      justify-content:center;
      align-items:center;
    }
    .mobile-socialmedia-container{
      height:9rem;
      width:100%;
      background-color:#141414;


      display:flex;
      flex-direction:row;
      justify-content:space-evenly;
      align-items:center;
    }
    .mobile-copyright-container{
      height:6rem;  
      width:100%;
      background-color:#141414;


      display:flex;
      flex-direction:row;
      justify-content:center;
      align-items:center;

    }
    .footer-new-desktop{
      display:none;
    }
  }

  {% comment %} //////////////////DESK///////////////////// {% endcomment %}
  @media (min-width: 1100px) {
    .footer-mobile {
      display:none;
    }
    .footer-desktop {
          display:block;
    }
    .footer-new-desktop{
      padding-bottom: 3rem;
    border-bottom: 2px solid grey;
    }
    .new-desktop-titles{
margin:0px;
    }
    .new-desktop-titles p {
      font-size:2.8rem !important;
      margin:0px; 
    }
    .grid-container {
          display: grid;
          grid-template-columns: repeat(6, 1fr); /* 6 equal columns */
          grid-template-rows: 250px 250px; /* 2 rows */
          height:100%;
          
    }
    .grid-item {
      max-width: 190px;
      margin-top:4rem;  

      
    }
    /* Adjustments for item with number 3 */
    .item6 {
        grid-column: 5;          /* Place in the sixth column */
        grid-row: 1 / span 2;    /* Start at row 1 and span 2 rows */
    }

}
  

{%- endstyle -%}



<section class="newsletter-container page-width ">

  <div class="newsletter-image-container ">
    {%- if section.settings.image_newsletter != blank -%}
      <img
        srcset="{%- if section.settings.image_newsletter.width >= 165 -%}{{ section.settings.image_newsletter | image_url: width: 165 }} 165w,{%- endif -%}
          {%- if section.settings.image_newsletter.width >= 360 -%}{{ section.settings.image_newsletter | image_url: width: 360 }} 360w,{%- endif -%}
