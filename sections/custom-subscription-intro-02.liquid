.master-section-container{
  height:100vh;
  border:20px solid var(--padding-sections);
}
.master-title-subtitle-button-container{
  height:20vh;
  display:flex;
  flex-direction:row;
  justify-content:space-between;
  align-items:center;
  background-color:#141414;

}
.master-title-subtitle-container{
  width:50%;
}
.master-content-container{


  overflow-x: auto;
  -webkit-overflow-scrolling: touch; /* Enables momentum-based scrolling on iOS devices */
  scroll-snap-type: x mandatory; 


  display:flex;
  flex-direction:row;
  justify-content:space-between;
  align-items:center;

  background-color:var(--padding-sections);
  width:100%;
  height: calc(80vh - 10vh - 40px );
}
.content-box-triple{
  max-width:33.33%;
  min-width:400px;

  padding:0px;


  display:flex;
  flex-direction:column;
  justify-content:space-between;
  text-align:center;
  align-items:center;
  scroll-snap-align: start; 
}
.master-image{
  width:100%;
  height:60%;
  object-fit:cover;
}
.text-content-container{
  background-color:#141414;
  padding:20px;

  display:flex;
  flex-direction:column;
  justify-content:start;
