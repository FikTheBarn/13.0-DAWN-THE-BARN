  letter-spacing: 2px;
}
.text-content-container-past_noimage h3{
  font-family: var(--font-main);
  font-size: var(--font-size-medium-small);
  margin:0px;
  padding:0px;
  line-height: normal;
  letter-spacing: 2px;

}
.text-content-container-past_noimage h4{
  margin:0px;
  padding:0px;
  line-height: normal;
}
.text-content-container-past_noimage  p{
  font-family:var(--font-secondary);
  font-size:var(--font-size-small);
  margin:0px;
  padding:0px;
  line-height: normal;
}
.button-link  {
  text-decoration: none; /* Removes underline from links */
  color: inherit; /* Ensures text color inherits from parent elements */
}
.button-main a  {
  text-decoration: none; /* Removes underline from links */
  color: white;

}
.button-main p  {

  margin:0px;
  padding:0px;
}

.button-main {
  background-color: var(--brown);
  padding: 1.6rem;
  border-radius: 5px;
  color: var(--white);
  min-width: 20rem; 
  font-size: 16px;
  border: 2px solid black;
  transition: box-shadow 0.3s ease-in-out; /* Smooth transition for the shadow */
  cursor: pointer; /* Ensures the cursor changes to a pointer when hovered */
}

.button-main:hover {
  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.9);
  transform: scale(1.05); /* Slightly enlarges the button */
}




