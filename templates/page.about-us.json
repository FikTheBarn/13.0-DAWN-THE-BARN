    "rich_text_t4MBCD": {
      "type": "rich-text",
      "blocks": {
        "heading_XzkNDV": {
          "type": "heading",
          "settings": {
            "heading": "Roast Style",
            "heading_size": "h1"
          }
        },
        "text_fqUGyj": {
          "type": "text",
          "settings": {
            "text": "<p>Our roast style is fully developed but light and clean. We bring out the full flavour potential of each coffee. Working with Single Origin Beans is a lot easier than using Blends - all beans are uniform and of highest grading.<\/p>"
          }
        }
      },
      "block_order": [
